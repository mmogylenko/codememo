{
  "name": "codememo",
  "version": "1.0.2",
  "description": "An interactive coding environment. You can write Javascript, import any NPM modules and see it executed, and write comprehensive documentation using markdown.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": "dist/index.js",
  "scripts": {
    "start": "tsc --watch --preserveWatchOutput",
    "prepublishOnly": "esbuild src/index.ts --platform=node --outfile=dist/index.js --bundle --minify --define:process.env.NODE_ENV=\\\"production\\\""
  },
  "keywords": [
    "codememo",
    "editor",
    "notebook",
    "playground"
  ],
  "author": "Mykola Mogylenko",
  "license": "ISC",
  "dependencies": {
    "@codememo/local-client": "^1.0.2"
  },
  "devDependencies": {
    "@codememo/local-api": "^1.0.2",
    "@types/node": "^17.0.40",
    "commander": "^9.3.0",
    "esbuild": "^0.14.42",
    "open": "^8.4.0",
    "typescript": "^4.7.3"
  }
}
